.form-row label{
	width:30%;
	color: #3D3D3D;
}
.app-form-control{
	width: 65%;
	padding: 4px 10px;
	border: 0;
}
.app-form-control:focus,
.app-select:focus{
	outline: 0;
}
.customer-id span{
	font-size: 31px;
	color: #29A0D5;
}
.form-row {
	/*margin-bottom: 10px;*/
	border-bottom: 1px solid #c7c7c7;
	padding: 5px 10px;
	min-height: 40px;
}
.form-row:last-child{
	border: 0;
}
.tab-content{
	background: #F8F8F8;
	padding: 10px;
}
.tab-content h4{
	padding-left: 10px;
}
.form-group{
	border: 1px solid #ADADAD;
	background: #fff;
	box-shadow: 1px 2px 0px 0px #fff;
	border-radius: 10px;
}
.no-form-group{
	border: 1px solid #ADADAD;
	border-top: 0;
	border-bottom: 0;
	background: #fff;
	box-shadow: 1px 2px 0px 0px #fff;
	border-radius: 0px;
}
.app-select{
	width: 100%;
	border: 0;
	font-size: 16px;
	padding: 3px 5px;
	width: 65%;
}
.report-table{
	margin: -10px;
	margin-top: 10px;
}
.report-table table th{
	border-bottom: 5px solid #29A0D5;
	background: -webkit-radial-gradient(circle, #69B9Dc, #259ED4);
	padding: 5px 0px;
	text-align: left;
	color: #1A1A1A;
	padding-left: 10px;
}
.report-pdf-table table th{
	background: none;
}
.report-table table td{
	border-left: 1px solid #29A0D5;
	padding: 4px 0px;
	text-align: left;
	color: #373737;
	padding-left: 10px;
}
.report-table table td:first-child{
	border-left: 0px solid #29A0D5;
}
.report-table table tr:last-child td{
	border-bottom: 5px solid #29A0D5;
	color: #373737;
	border-top: 1px solid #29A0D5;
	text-transform: uppercase;
}
.report-table th:first-child,
.report-table td:first-child {
	text-align: left;
	padding-left: 10px;
}
.landing-body{
	background: -webkit-radial-gradient(circle, #69B9Dc, #259ED4);
	display: table;
}
.landing-wrapper{
	display: table-cell;
	vertical-align: middle;
	padding: 30px;
	width: 100%;
	text-align: center;
}
.logo{
	width: 50%;
}
.loader{
	position: absolute;
	bottom: 20px;
	left: 50%;
}
.loader img{
	position: relative;
	left: -50%;
}
.pdf-body{
	background-color: #EFEFEF;
	padding: 10px;
}
.pdf-wrapper h1{
	text-align: center;
	text-align: center;
	color: #444444;
	font-weight: lighter;
}
.customer-details label{
	font-weight: bold;

}
.button-area{
	text-align: center;
}
.button-area input,
.button-area a{
	text-decoration: none;
	background: #29A0D5;
	border: 0;
	padding: 10px 20px;
	color: #fff;
	/*margin-bottom: 5px;*/
	display: inline-block;
}
.button-area input:focus,
.button-area input:hover,
.button-area a:hover{
	background-color: rgba(41, 160, 213, 0.75);
}
.add-btn{
	margin-left: 10px;
	font-size: 20px;
}
.signature{
	border: 1px solid #c7c7c7;
	margin: 20px 0;
}
.signature canvas{
	height: 180px !important;
}
.id-images img{
	width: 80px;
	border: 1px solid #c7c7c7;
	border-radius: 15px;
	height: auto;
}
.toggle-on.btn{
	color: #fff;
}
.table-responsive{
	margin: 0px !important;
	border: 0 !important;
}
.table-striped tr:last-child td{
	border-top: 0px !important;
}
.glyphicon-barcode {
  position: absolute;
  right: 0;
  padding: 8px 27px;
}
.glyphicon-calendar {
  position: absolute;
  right: 0;
  padding: 8px 27px;
}