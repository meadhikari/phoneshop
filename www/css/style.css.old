html, body{
	color: #000;
	height: 100%;
	margin: 0;
	font-family: helvetica;
}
body{
	min-height: 100%;
}
*{
	box-sizing: border-box;
}
.login-wrapper{
	position: relative;
	height: 100%;
	text-align: center;
	width: 100%;
	display: table;
	/*background:url('../images/bg.jpg');*/
	background: -webkit-radial-gradient(circle, #69B9Dc, #259ED4);
	background-repeat: no-repeat;
	background-size: cover;
	padding: 20px;
}
.login-content{
	min-height: 100%;
	display: table-cell;
	vertical-align: middle;
}
.login-form-control{
	display: block;
	padding: 8px 15px;
	margin: 15px 0;
	width: 100%;
	border: 1px solid #c7c7c7;
	border-radius: 17px;
	box-shadow: 2px 4px 0px rgba(0, 0, 0, 0.3);
	height: 34px;
}
.login-logo img{
	width: 40%;
}
.login-logo h1{
	color: #fff;
}
.login-btn{
	display: block;
	padding: 8px 15px;
	margin: 15px 0;
	width: 50%;
	border: 1px solid #c7c7c7;
	border-radius: 17px;
	box-shadow: 2px 4px 0px rgba(0, 0, 0, 0.3);
	font-weight: bold;
	color: #29A0D5;
	background: rgba(255, 255, 255, 1);
	height: 34px;
}
.login-btn:hover{
	background: rgba(255, 255, 255, 0.9);
}
.login-wrapper p{
	text-align: left;
}
.login-wrapper a{
	color: #fff;
	text-decoration: none;
}

/*nav tabs*/

.nav-tabs{
	border: 0;
	border-radius: 0;
}
.nav-tabs > li > a{
	border: 0;
	border-radius: 0;
	font-size: 17px !important;
	color: #fff !important;
	padding-left: 0;
	padding-right: 0;
}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus{
	border: 0;
	border-radius: 0;
	color: #29A0D5 !important;
	background-color: rgba(255, 255, 255, 0.64);
}
.nav > li > a:hover, .nav > li > a:focus{
	background-color: rgba(255, 255, 255, 0.64);
}
.nav-tabs > li{
	width: 25%;
	text-align: center;
}
li a span{
	display: block !important;
	font-size: 20px;
	/*margin-bottom: 7px;*/
}
.nav-tabs{
	border-bottom: 5px solid #29A0D5;
	padding-bottom: 1px;
	/*background:url('../images/bg.jpg');*/
	background: -webkit-radial-gradient(circle, #69B9Dc, #259ED4);
	min-height: 100%;
	background-repeat: no-repeat;
	background-size: cover;
}
.home-wrapper{
	background-color: #F8F8F8;
	min-height: 100%;
}
